{
  "name": "fucking-web-application",
  "namespace": false,
  "jsx": {
    "version": 4
  },
  "bs-dependencies": ["@rescript/react", "@rescript/core", "@rescript/webapi"],
  "bsc-flags": ["-open RescriptCore", "-open WebApi.Global"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true,
    "suffix": ".js"
  },
  "external-stdlib": "@rescript/std",
  "uncurried": true,
  "suffix": ".js",
  "warnings": {
    "error": "+110+5"
  },
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "bundler",
    "generatedFileExtension": ".tsx",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "reanalyze": {
    "transitive": true
  }
}
